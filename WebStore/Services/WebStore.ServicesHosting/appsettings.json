{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=WebStore;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {

        "Microsoft": "Error",
        "System": "Error",
        "Default": "Information"
      }
    },
      "LogLevel": {
        "Default": "Warning"
      }
    },
  "AllowedHosts": "*"
}
